{
  "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "rgName": {
      "type": "string",
      "metadata": {
        "description": "Resource Group all resources will deploy to."
      }
    },
    "rgLocation": {
      "type": "string",
      "metadata": {
        "description": "Location all resources deployed to."
      }
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.Resources/resourceGroups",
      "apiVersion": "2021-04-01",
      "name": "[parameters('rgName')]",
      "location": "[parameters('rgLocation')]",
      "properties": {}
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "name": "GenerateArchiveStorage",
      "resourceGroup": "[parameters('rgName')]",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/resourceGroups',parameters('rgName'))]"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri":"https://raw.githubusercontent.com/kozloffj/Rackspace/main/IaCWebApplicationDemo/GenerateArchiveStorage/GenerateArchiveStorage.json",
          "contentVersion":"1.0.0.0"
        },
        "parametersLink": {
          "uri": "https://raw.githubusercontent.com/kozloffj/Rackspace/main/IaCWebApplicationDemo/GenerateArchiveStorage/GenerateArchiveStorage.parameters.json",
          "contentVersion": "1.0.0.0"
        }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "name": "DeployVMSSSample",
      "resourceGroup": "[parameters('rgName')]",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/resourceGroups',parameters('rgName'))]"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri":"https://raw.githubusercontent.com/kozloffj/Rackspace/main/IaCWebApplicationDemo/SampleVMSSWithWeb/azuredeploy.json",
          "contentVersion":"1.0.0.0"
        },
        "parametersLink": {
          "uri": "https://raw.githubusercontent.com/kozloffj/Rackspace/main/IaCWebApplicationDemo/SampleVMSSWithWeb/azuredeploy.parameters.json",
          "contentVersion": "1.0.0.0"
        }
      }
    }
  ],
  "outputs": {}
}
{
    "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "rgNameLogs": {
            "type": "string"
        },
        "rgNameWeb": {
            "type": "string"
        },
        "rgLocation": {
            "type": "string"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Resources/resourceGroups",
            "apiVersion": "2021-04-01",
            "location": "[parameters('rgLocation')]",
            "name": "[parameters('rgNameLogs')]",
            "properties": {}
        },
        {
            "type": "Microsoft.Resources/resourceGroups",
            "apiVersion": "2021-04-01",
            "location": "[parameters('rgLocation')]",
            "name": "[parameters('rgNameWeb')]",
            "properties": {}
        }
    ],
    "outputs": {
        "rgNameLogs": {
            "type": "string",
            "value": "[reference(parameters('rgNameLogs'))]"
        },
        "rgNameWeb": {
            "type": "string",
            "value": "[reference(parameters('rgNameWeb'))]"
        },
        "IaClocation": {
            "type": "string",
            "value": "[reference(parameters('rgLocation'))]"
        }
    }
}